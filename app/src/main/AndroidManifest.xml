<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.abc.driverbooking">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="in.wptrafficanalyzer.locationroutedirectionmapv2.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />



    <application

        android:allowBackup="true"
        android:icon="@drawable/driver_app_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/driver_app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.HelpActivity"
            android:label="Privacy Policy"
            android:parentActivityName="com.driver.driverbooking.MapActivity.MapsActivity" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.MapActivity.MapsActivity"
            android:label="@string/title_activity_maps"

            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.LoginDiverActivity"
            android:theme="@style/AppTheme.NoActionBar">


        </activity>
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.ProfileActivity"
            android:label="My Profile "
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.MyTripsActivity"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.Future_BookingsActivity"
            android:label="Future Booking"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.EditProfileActivity"
            android:parentActivityName="com.driver.driverbooking.MainActivity" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.DocumentsActivity"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:noHistory="true"
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.SplashScreen.SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.Forget_Password.ForgetMobilenoActivity"
            android:label="Forgot Password"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.Forget_Password.ForgetOtpActivity"
            android:label="Forgot Password"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.Forget_Password.ForgetUpdarePassActivity"
            android:label="Forgot Password"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.ContactUsActivity"
            android:label="Contact Us"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.AboutAsActivity"
            android:label="About As"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.ResetPasswordActivity"
            android:label="Reset Password"
            android:parentActivityName="com.driver.driverbooking.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.HistoryMyTripActivity" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:icon="@drawable/driver_app_icon"
            android:screenOrientation="portrait"
            android:name="com.driver.driverbooking.Activity.DriverTermsConditionActivity"/>
    </application>

</manifest>